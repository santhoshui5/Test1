<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="Root.webapp.controller.view3"
	xmlns:f="sap.ui.layout.form"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="View3" showNavButton="true" navButtonPress="onBack">
				<content>
					<f:SimpleForm items="{/supplier}">
						<f:content>
							<Label text="Name"></Label>
							<Text text="{name}"></Text>
							<Label text="Contact Person"></Label>
							<Text text="{contactPerson}"></Text>
							<Label text="City"></Label>
							<Text text="{city}"></Text>
							<Label text="Since When"></Label>
							<Text text="sinceWhen"></Text>
						</f:content>
					</f:SimpleForm>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>